<footer class="navfooter bg-light {{ app.request.pathInfo == "/connexion" ? "fixed-bottom" : "sticky-bottom" }}">
        <div>
                <a href="{{ path('home') }}"><img src="{{ asset('images/home.png') }}" alt=""></a>
        </div>
        <div>
                <img src="{{ asset('images/signet.png') }}" width="48" alt="">
        </div>
        {% if not app.user %}
        <div>
                <a href="{{ path('app_login') }}"><img src="{{ asset('images/entrer.png') }}" alt=""></a>
        </div>
        {% else %}
        <div>
                <a href="{{ path('app_logout') }}"><img src="{{ asset('images/sortie.png') }}" alt=""></a>
        </div>
        {% endif %}
</footer>